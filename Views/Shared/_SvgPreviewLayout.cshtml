    <title>@ViewData["Title"] - SvgOutputSample</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/svgpreview.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SvgOutputSample.styles.css" asp-append-version="true" />
</head>
<body>
    
    <div class="">
        <main >
